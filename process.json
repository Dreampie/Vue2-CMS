{
  "apps": [
    {
      "name": "cms-view",
      "script": "server.js",
      "error_file": "/opt/logs/cms-view/cms-view.error.log",
      "out_file": "/opt/logs/cms-view/cms-view.out.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "instances": "0",
      "watch": "false",
      "ignoreWatch": [
        "[\\/\\\\]\\./",
        "node_modules"
      ],
      "merge_logs": true,
      "exec_mode": "cluster_mode",
      "node_args": "--harmony --use-strict",
      "env_dev": {
        "NODE_ENV": "dev"
      },
      "env_devTest": {
        "NODE_ENV": "devTest"
      },
      "env_test": {
        "NODE_ENV": "test"
      },
      "env_staging": {
        "NODE_ENV": "staging"
      },
      "env_prod": {
        "NODE_ENV": "prod"
      }
    }
  ]
}